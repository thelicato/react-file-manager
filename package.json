{
  "name": "@thelicato/react-file-manager",
  "description": "A simple React component to build a Web File Manager",
  "version": "1.1.5",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "run-s build:*",
    "build:lib": "tsup lib/index.ts --dts",
    "build:css": "tailwindcss build -i lib/tailwind.css -o dist/style.css",
    "test": "vitest run",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "format": "prettier --write .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@tanstack/react-table": "^8.16.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-draggable": "^4.4.6",
    "react-dropzone": "^14.2.3"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.3.2",
    "@frontside/eslint-config": "^3.1.0",
    "@storybook/addon-essentials": "^8.0.8",
    "@storybook/addon-interactions": "^8.0.8",
    "@storybook/addon-links": "^8.0.8",
    "@storybook/addon-mdx-gfm": "^8.0.8",
    "@storybook/addon-onboarding": "^8.0.8",
    "@storybook/blocks": "^8.0.8",
    "@storybook/react": "^8.0.8",
    "@storybook/react-vite": "^8.0.8",
    "@storybook/test": "^8.0.8",
    "@testing-library/react": "^15.0.2",
    "@types/inquirer": "^9.0.3",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/recursive-readdir": "^2.2.1",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@vitejs/plugin-react": "^3.0.1",
    "autoprefixer": "^10.4.19",
    "chalk": "^5.2.0",
    "esbuild-css-modules-plugin": "^2.7.1",
    "eslint": "^8.33.0",
    "eslint-plugin-storybook": "^0.8.0",
    "fetch-lite": "^1.1.0",
    "inquirer": "^9.1.4",
    "kolorist": "^1.6.0",
    "lightningcss": "^1.24.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "prettier": "^2.8.4",
    "prop-types": "^15.8.1",
    "recursive-readdir": "^2.2.3",
    "rollup-plugin-postcss": "^4.0.2",
    "storybook": "^8.0.8",
    "tailwindcss": "^3.4.3",
    "ts-node": "^10.9.1",
    "tsup": "^6.5.0",
    "type-fest": "^3.5.4",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vite-plugin-css-injected-by-js": "^2.4.0",
    "vite-plugin-dts": "^1.7.1",
    "vitest": "^0.28.4"
  },
  "bugs": {
    "url": "https://github.com/thelicato/react-file-manager/issues"
  }
}
